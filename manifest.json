{
  "name": "NotificationhubToChromeApp",
  "description": "Node.js Chatting Chrom App with Azure Notification Hub.",
  "manifest_version": 1,
  "version": "0.1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": ["gcm", "storage", "notifications", "https://*.servicebus.windows.net/*", "webview"],
  "icons": { "128": "gcm_128.png" }
}